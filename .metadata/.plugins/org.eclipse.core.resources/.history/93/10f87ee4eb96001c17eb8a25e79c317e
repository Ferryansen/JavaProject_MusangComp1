package parents;

public class father extends parent {
	private String name;
	private int umur;
	
	public father(String name, int umur, int strictness, String hasHouse) {
		super(strictness, hasHouse);
		this.name = name;
		this.umur = umur;
		
		System.out.println("Nama ayahmu adalah " + name);
		System.out.println("Dia sudah berumur " + umur);
	}
}
